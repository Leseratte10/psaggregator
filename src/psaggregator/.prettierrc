{
    "useTabs": false,
    "tabWidth": 4,
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 100,
    "bracketSameLine": true,
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "overrides": [{
        "files": "*.svelte",
        "options": {
            "parser": "svelte"
        }
    }]
}